                        var swiper = new Swiper(".mySwiper", {
                            loop: true,
                            centeredSlides: false,
                            grabCursor: true,
                            autoplay: {
                                delay: 2000
                            },
                            navigation: {
                                nextEl: ".swiper-button-next",
                                prevEl: ".swiper-button-prev",
                            },
                            breakpoints: {
                                480: {
                                    slidesPerView: 2,
                                    slidesPerGroup: 1,
                                },
                                760: {
                                    slidesPerView: 3,
                                    slidesPerGroup: 2,
                                },
                                1200: {
                                    slidesPerView: 4,
                                    slidesPerGroup: 2,
                                },
                            }
                        });
