                    </div>
                    <script>                            
                        const profilePopupImages = [...document.querySelectorAll('.prof_img')];
                        const profilePopup = document.querySelector('.profile_img_popup');
                        const popupClose = document.querySelector('.profile_img_popup .btn_close');
                        const popupImage = document.querySelector('.img_popup-large');
                        const prevButton = document.querySelector('.profile_img_popup .ic_arrow.left');
                        const nextButton = document.querySelector('.profile_img_popup .ic_arrow.right');

                        let index = 0;
                        profilePopupImages.forEach(function(item, i) {
                            item.addEventListener('click', function() {
                                profilePopup.classList.toggle('active');
                                updateImage(i);
                                document.querySelector('body').style.overflow = 'hidden';
                            });
                        });
                        const updateImage = function(i) {
                            let path = `/res/imgs/artist/leeyoonje/leeyoonje_${i+1}.jpg`;
                            popupImage.src = path;
                            index = i;
                        }
                        popupImage.addEventListener('click', function() {
                            profilePopup.classList.toggle('active');
                            document.querySelector('body').style.overflow = 'auto';
                        });
                        popupClose.addEventListener('click', function() {
                            profilePopup.classList.toggle('active');
                            document.querySelector('body').style.overflow = 'auto';
                        });
                        prevButton.addEventListener('click', function() {
                            index = (index - 1 + profilePopupImages.length) % profilePopupImages.length;
                            updateImage(index);
                        });
                        nextButton.addEventListener('click', function() {
                            index = (index + 1) % profilePopupImages.length;
                            updateImage(index);
                        });
                    </script>
                </div>
