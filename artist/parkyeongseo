                        <script>                            
                            const profileActivitiesPopupImages = [...document.querySelectorAll('.prof_activities_img')];
                            const profilActivitiesePopup = document.querySelector('.profile_activities_img_popup');
                            const activitiesPopupClose = document.querySelector('.profile_activities_img_popup .btn_close');
                            const activitiesPopupImage = document.querySelector('.activities_img_popup-large');
                            const activitiesPrevButton = document.querySelector('.profile_activities_img_popup .ic_arrow.left');
                            const activitiesNextButton = document.querySelector('.profile_activities_img_popup .ic_arrow.right');

                            let activitiesIndex = 0;
                            profileActivitiesPopupImages.forEach(function(item, i) {
                                item.addEventListener('click', function() {
                                    profilActivitiesePopup.classList.toggle('active');
                                    activitieUpdateImage(i);
                                    document.querySelector('body').style.overflow = 'hidden';
                                });
                            });
                            const activitieUpdateImage = function(i) {
                                let path = `/res/imgs/artist/parkyeongseo/parkyeongseo_activities_${-(i - profileActivitiesPopupImages.length)}.jpg`;
                                activitiesPopupImage.src = path;
                                activitiesIndex = i;
                            }
                            activitiesPopupImage.addEventListener('click', function() {
                                profilActivitiesePopup.classList.toggle('active');
                                document.querySelector('body').style.overflow = 'auto';
                            });
                            activitiesPopupClose.addEventListener('click', function() {
                                profilActivitiesePopup.classList.toggle('active');
                                document.querySelector('body').style.overflow = 'auto';
                            });
                            activitiesPrevButton.addEventListener('click', function() {
                                activitiesIndex = (activitiesIndex - 1 + profileActivitiesPopupImages.length) % profileActivitiesPopupImages.length;
                                activitieUpdateImage(activitiesIndex);
                            })
                            activitiesNextButton.addEventListener('click', function() {
                                activitiesIndex = (activitiesIndex + 1) % profileActivitiesPopupImages.length;
                                activitieUpdateImage(activitiesIndex);
                            });
                        </script>
                    </div>
                    <!-- Swiper JS -->
                    <script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>

                    <!-- Initialize Swiper -->
                    <script>
                        var swiper = new Swiper(".mySwiper", {
                            loop: true,
                            centeredSlides: false,
                            grabCursor: true,
                            autoplay: {
                                delay: 2000
                            },
                            navigation: {
                                nextEl: ".swiper-button-next",
                                prevEl: ".swiper-button-prev",
                            },
                            breakpoints: {
                                480: {
                                    slidesPerView: 2,
                                    slidesPerGroup: 1,
